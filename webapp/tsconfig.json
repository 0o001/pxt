{
    "compilerOptions": {
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "module": "commonjs",
        "outDir": "built",
        "rootDir": ".",
        "newLine": "LF",
        "sourceMap": true,
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "preserveConstEnums": true
    },
    "exclude": [ "node_modules" ]
}
