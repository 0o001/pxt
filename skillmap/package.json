{
  "name": "skillsmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.19.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "mv node_modules/react-scripts/config/webpack.config.js node_modules/react-scripts/config/webpack.config.base.js && cp webpack.config.override.js node_modules/react-scripts/config/webpack.config.js",
    "build": "react-scripts build",
    "postbuild": "mv node_modules/react-scripts/config/webpack.config.base.js node_modules/react-scripts/config/webpack.config.js && rm build/static/js/runtime* && rm build/static/js/*.txt"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
